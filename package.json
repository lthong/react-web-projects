{
  "name": "react-web-projects",
  "version": "1.0.0",
  "description": "Some web projects for react development",
  "main": "index.js",
  "author": "Karen Hong",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/lthong/react-web-projects",
  "dependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.12.13",
    "@material-ui/core": "^4.11.0",
    "autoprefixer": "^10.0.1",
    "axios": "^0.20.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "clsx": "^1.1.1",
    "css-loader": "^4.2.1",
    "decimal.js": "^10.2.1",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.3.0",
    "i18next": "^19.8.2",
    "immutable": "^4.0.0-rc.12",
    "mini-css-extract-plugin": "^0.12.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.4",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.11.3",
    "react-icons": "^4.3.1",
    "react-immutable-proptypes": "^2.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config",
    "build": "webpack --config webpack.prod.config",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "react/prop-types": "off",
      "no-unused-vars": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint"
  }
}
